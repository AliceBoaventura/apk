<?xml version="1.0" encoding="utf-8"?>
<manifest
   package="com.wchat" xmlns:android="http://schemas.android.com/apk/res/android">
<application
    android:allowBackup="true"
    android:label="@string/app_name"
    android:icon="@mipmap/ic_launcher"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.WeChatCompanion">
    <activity android:name=".RedEnvelopePredictorActivity" />
    <activity android:name=".WeChatModActivity" />
    <activity android:name=".PrototypeActivity" />
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
</application>


</manifest>